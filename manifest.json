{
    "manifest_version": 2,
    "name": "Hash-Checker",
    "version": "1.0",
    "description": "Make webapp a app",
    "icons": {
        "128": "icon.png"
    },
    "permissions": [
        "webRequest",
        "tabs",
        "webRequestBlocking",
        "storage",
        "<all_urls>"
    ],
    "page_action": {
        "browser_style": true,
        "default_icon": {
          "19": "./assets/icons/wa-16_text_inact.png",
          "38": "./assets/icons/wa-32_text_inact.png"
        },
        "default_title": "__MSG_PageActionTitleN__"
      },
    "background": {
        "scripts": [
            "background/sha256.js",
            "background/main.js"
        ]
    }
}